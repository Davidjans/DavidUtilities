/* Light.uss */
:root {
    font-size: 7px;
    color: #333;
    background-color: #00FF00;
}
Label {
    color: #FFFF00;
    margin-bottom: 4px;
}
Button {
    background-color: #ff00cc;
    color: #333;
    border: 1px solid #CCC;
    border-radius: 2px;
    padding: 5px 10px;
    font-size: 19;
}
Toggle, Slider {
    margin: 4px 0;
}
Box {
    border: 1px solid #0000FF;
    border-radius: 4px;
    padding: 5px;
    margin-top: 5px;
    background-color: #FF0000;

    font-size: 28;
}
