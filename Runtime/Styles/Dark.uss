/* Dark.uss */
:root {
    font-size: 12px;
    color: #FFF;
    background-color: #333;
}
Label {
    color: #FFF;
    margin-bottom: 4px;
}
Button {
    background-color: #555;
    color: #FFF;
    border: 1px solid #444;
    border-radius: 2px;
    padding: 5px 10px;
}
Toggle, Slider {
    margin: 4px 0;
}
Box {
    border: 1px solid #666;
    border-radius: 4px;
    padding: 5px;
    margin-top: 5px;
    background-color: #444;
}

